<idea-plugin>
    <id>falcon-jetbrains-themes</id>
    <version>6.0.0</version>
    <name>Falcon Relaxing-Eyes Islands Themes - Light/Dark</name>
    <vendor email="xiaoan.pan@qq.com" url="https://github.com/panxiaoan">Panxiaoan</vendor>
    <category>UI</category>

    <!-- 最低支持 IDEA 版本：2023.2 https://plugins.jetbrains.com/docs/intellij/build-number-ranges.html#earlier-versions -->
    <idea-version since-build="232"/>

    <depends>com.intellij.modules.platform</depends>

    <!--<applicationListeners>
        <listener class="com.falcon.jetbrains.themes.FalconThemesChangeListener"
                  topic="com.intellij.ide.ui.LafManagerListener"/>
    </applicationListeners>-->

    <extensions defaultExtensionNs="com.intellij">
        <!-- Islands Light Theme -->
        <themeProvider id="falcon-relax-islands-light-celadon" path="/themes/islands-light/islands-celadon.theme.json"/>
        <themeProvider id="falcon-relax-islands-light-green" path="/themes/islands-light/islands-green.theme.json"/>
        <themeProvider id="falcon-relax-islands-light-green-contrast" path="/themes/islands-light/islands-green-contrast.theme.json"/>
        <themeProvider id="falcon-relax-islands-light-bean-green" path="/themes/islands-light/islands-bean-green.theme.json"/>
        <themeProvider id="falcon-relax-islands-light-grey" path="/themes/islands-light/islands-grey.theme.json"/>
        <themeProvider id="falcon-relax-islands-light-buff" path="/themes/islands-light/islands-buff.theme.json"/>
        <themeProvider id="falcon-relax-islands-light-pink" path="/themes/islands-light/islands-pink.theme.json"/>
        <themeProvider id="falcon-relax-islands-light-yellow" path="/themes/islands-light/islands-yellow.theme.json"/>

        <!-- Islands Dark Theme -->
        <themeProvider id="falcon-relax-islands-dark" path="/themes/islands-dark/islands-dark.theme.json"/>
        <themeProvider id="falcon-relax-islands-dark-blue" path="/themes/islands-dark/islands-dark-blue.theme.json"/>
        <themeProvider id="falcon-relax-islands-dark-coffee" path="/themes/islands-dark/islands-dark-coffee.theme.json"/>
        <themeProvider id="falcon-relax-islands-dark-darcula" path="/themes/islands-dark/islands-dark-darcula.theme.json"/>
        <themeProvider id="falcon-relax-islands-dark-green" path="/themes/islands-dark/islands-dark-green.theme.json"/>
        <themeProvider id="falcon-relax-islands-dark-violet" path="/themes/islands-dark/islands-dark-violet.theme.json"/>

        <!-- Light Theme -->
        <themeProvider id="falcon-relax-light-buff" path="/themes/light/buff.theme.json"/>
        <themeProvider id="falcon-relax-light-celadon" path="/themes/light/celadon.theme.json"/>
        <themeProvider id="falcon-relax-light-green" path="/themes/light/green.theme.json"/>
        <themeProvider id="falcon-relax-light-green-contrast" path="/themes/light/green-contrast.theme.json"/>
        <themeProvider id="falcon-relax-light-grey" path="/themes/light/grey.theme.json"/>
        <themeProvider id="falcon-relax-light-pink" path="/themes/light/pink.theme.json"/>
        <themeProvider id="falcon-relax-light-bean-green" path="/themes/light/bean-green.theme.json"/>
        <themeProvider id="falcon-relax-light-yellow" path="/themes/light/yellow.theme.json"/>

        <!-- Dark Theme -->
        <themeProvider id="falcon-relax-dark" path="/themes/dark/dark.theme.json"/>
        <themeProvider id="falcon-relax-dark-blue" path="/themes/dark/dark-blue.theme.json"/>
        <themeProvider id="falcon-relax-dark-coffee" path="/themes/dark/dark-coffee.theme.json"/>
        <themeProvider id="falcon-relax-dark-darcula" path="/themes/dark/dark-darcula.theme.json"/>
        <themeProvider id="falcon-relax-dark-green" path="/themes/dark/dark-green.theme.json"/>
        <themeProvider id="falcon-relax-dark-violet" path="/themes/dark/dark-violet.theme.json"/>
    </extensions>

</idea-plugin>